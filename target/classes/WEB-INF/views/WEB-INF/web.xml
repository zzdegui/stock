<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <filter>
    <filter-name>loginF</filter-name>
    <filter-class>customer.web.LoginFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>loginF</filter-name>
    <url-pattern>*.jhtml</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <filter>
    <filter-name>PaymentFilter</filter-name>
    <filter-class>com.xmall.payment.web.filter.PaymentFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>PaymentFilter</filter-name>
    <url-pattern>*.jhtml</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <filter>
    <filter-name>urlRewrite</filter-name>
    <filter-class>j.w.JSPFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>urlRewrite</filter-name>
    <url-pattern>*.jhtml</url-pattern>
    <dispatcher>REQUEST</dispatcher>
    <dispatcher>FORWARD</dispatcher>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>